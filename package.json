{
  "name": "fabric-store-monorepo",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "cd frontend && pnpm install && pnpm build && cd ../backend && pnpm install",
    "dev:frontend": "cd frontend && pnpm dev",
