    "dev": "pnpm -r dev",
    "start": "cd backend && pnpm start",
    "seed": "cd backend && pnpm seed"
  },
  "dependencies": {},
  "devDependencies": {},
  "pnpm": {
    "overrides": {
      "vite": "6.3.5"
    }
  }
}